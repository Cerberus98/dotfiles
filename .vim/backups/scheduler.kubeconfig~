apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: /etc/ssl/ca.crt
    server: https://{{ hosts[0].host_ip }}:6443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: scheduler
  name: scheduler-ctx
current-context: scheduler-ctx
users:
- name: scheduler 
  user:
    client-certificate: /etc/ssl/server.cert
    client-key: /etc/ssl/server.key
