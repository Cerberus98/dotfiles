apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: /etc/ssl/ca.crt
    server: https://{{ hosts[0].host_ip }}:6443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: proxy
  name: proxy-ctx
current-context: proxy-ctx
users:
- name: proxy
  user:
    client-certificate: /etc/ssl/server.crt
    client-key: /etc/ssl/server.key
